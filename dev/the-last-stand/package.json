{
  "name": "the-last-stand-v2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "client": "vite",
    "server": "ts-node-dev --respawn --transpile-only --project tsconfig.server.json src/server/main.ts",
    "build": "tsc && vite build",
    "build-yolo": "vite build",
    "preview": "vite preview",
    "loadtest-server": "colyseus-loadtest loadtest/example.ts --room my_room --numClients 2",
    "build-server": "npm run clean && tsc && node node_modules/copyfiles/copyfiles package.json ./lib && node node_modules/copyfiles/copyfiles arena.env ./lib",
    "clean-server": "node node_modules/rimraf/bin lib",
    "test-server": "mocha --require ts-node/register test/**_test.ts --exit --timeout 15000"
  },
  "build": {
    "chunkSizeWarningLimit": 100000
  },
  "dependencies": {
    "@colyseus/arena": "^0.14.22",
    "@colyseus/command": "^0.2.1",
    "@colyseus/monitor": "^0.14.20",
    "colyseus": "^0.14.20",
    "colyseus.js": "^0.14.13",
    "connect-mongo": "^4.6.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-session": "^1.17.3",
    "mongodb": "^5.1.0",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "phaser": "^3.55.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-router-dom": "^6.8.2"
  },
  "devDependencies": {
    "@colyseus/loadtest": "^0.14.4",
    "@colyseus/testing": "^0.14.21",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.6",
    "@types/mocha": "^8.2.3",
    "@types/node": "^18.14.6",
    "@types/passport": "^1.0.12",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.13",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.35.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^9.0.2",
    "postcss-cli": "^10.1.0",
    "prettier": "^2.8.4",
    "rimraf": "^2.7.1",
    "tailwindcss": "^3.2.7",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^1.0.0-pre.63",
    "typescript": "^4.9.5",
    "vite": "^4.1.0",
    "vite-plugin-env-compatible": "^1.1.1"
  }
}
